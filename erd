Department
-
dept_no VARCHAR(4) PK
dept_name VARCHAR(100)


DepartmentManager
-
dept_no VARCHAR(4) PK FK >- Department.dept_no
emp_no INT PK FK - Employee.emp_no
from_date DATE
to_date DATE


Employee
-
emp_no  PK INT 
birth_date DATE
first_name VARCHAR 
last_name VARCHAR
gender VARCHAR(1)
hire_date DATE

DepartmentEmployee
-
emp_no INT PK FK >- Employee.emp_no
dept_no VARCHAR(4) PK FK >- Department.dept_no
from_date DATE
to_date DATE

Salaries
-
emp_no INT  PK FK - Employee.emp_no
salary INT
from_date DATE
to_date DATE

Titles
-
emp_no INT  PK FK >- Employee.emp_no
title VARCHAR(50)
from_date DATE
to_date DATE


